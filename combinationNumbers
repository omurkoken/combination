import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
       
        int total=1;
        Scanner combine= new Scanner(System.in);
        System.out.print("faktoriyel alınacak sayı: ");
        int n=combine.nextInt();

        for (int i=1; i<=n; i++){
            total=total*i;
        }
        System.out.println(n+"'in faktöriyeli : " +total);
    }
}
